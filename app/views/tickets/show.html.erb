
<% if current_user.present? && current_user.kind == "doorman" %>
    <p class=""><%= link_to "<- Назад", tickets_path, class:"ms-text-light" %></p>
<% end %>
<h4>Тикет: <%= @ticket.token %> </h4>
<p>Емаил: <%= @ticket.email %></p>

<% if !@ticket.vazecka? %>
    <p class="ms-text-secondary">Улезел: <%= @ticket.updated_at + 2.hour %></p>
<% else %>
    <p class="ms-text-secondary">👇🏻 Покажи на влез</p>
<% end %>
